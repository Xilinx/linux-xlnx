&gem3 {
	clocks = <&clkc 31>, <&clkc 52>, <&clkc 48>, <&clkc 52>;
	clock-names = "pclk", "hclk", "tx_clk", "rx_clk";
};

&gpio {
	clocks = <&clkc 31>;
};

&i2c0 {
