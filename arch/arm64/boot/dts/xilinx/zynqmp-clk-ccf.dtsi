&gem0 {
	clocks = <&clkc 45>, <&clkc 45>, <&clkc 49>;
	clock-names = "pclk", "tx_clk", "hclk";
};

&gem1 {
	clocks = <&clkc 46>, <&clkc 46>, <&clkc 50>;
	clock-names = "pclk", "tx_clk", "hclk";
